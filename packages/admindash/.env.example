# NextAuth Configuration
NEXTAUTH_URL=https://admin.yanbrain.com
NEXTAUTH_SECRET=CHANGE_THIS_TO_RANDOM_SECRET_MINIMUM_32_CHARS

# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=YOUR_FIREBASE_API_KEY
NEXT_PUBLIC_FIREBASE_PROJECT_ID=yanbrainserver
NEXT_PUBLIC_CLOUD_FUNCTIONS_URL=https://us-central1-yanbrainserver.cloudfunctions.net/api

# Firebase Admin SDK (Server-side only - DO NOT EXPOSE TO CLIENT)
FIREBASE_PROJECT_ID=yanbrainserver
FIREBASE_CLIENT_EMAIL=YOUR_SERVICE_ACCOUNT_EMAIL
FIREBASE_PRIVATE_KEY="YOUR_PRIVATE_KEY_HERE"
